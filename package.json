{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "Safari >= 13",
      "iOS >= 13"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "gh": "cross-env BASE_PATH=/telegram_mini_app/ vite build --logLevel error && node ./scripts/write-version.mjs && npx gh-pages -d dist -b gh-pages",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.{ts,tsx,less,css,md}\" \"scripts/**/*.{mjs,js}\"",
    "preview": "vite preview",
    "images:webp": "node ./scripts/convert-images-to-webp.mjs",
    "images:webp:high": "WEBP_QUALITY=90 node ./scripts/convert-images-to-webp.mjs",
    "images:webp:lossless": "WEBP_LOSSLESS=true node ./scripts/convert-images-to-webp.mjs",
    "images:webp:recompress": "WEBP_QUALITY=75 EFFORT=6 MAX_WIDTH=1920 MAX_HEIGHT=1920 node ./scripts/convert-images-to-webp.mjs",
    "images:webp:resize": "MAX_WIDTH=1920 MAX_HEIGHT=1920 WEBP_QUALITY=75 node ./scripts/convert-images-to-webp.mjs",
    "images:optimize": "WEBP_QUALITY=75 EFFORT=6 MAX_WIDTH=1920 MAX_HEIGHT=1920 node ./scripts/convert-images-to-webp.mjs",
    "images:rename": "node ./scripts/rename-images.mjs"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^6.26.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@stylistic/eslint-plugin": "^2.13.0",
    "@types/node": "^22.18.3",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.23",
    "cross-env": "^7.0.3",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.3.0",
    "less": "4.4.1",
    "postcss": "^8.5.6",
    "postcss-flexbugs-fixes": "^5.0.2",
    "prettier": "^3.6.2",
    "sharp": "0.34.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.42.0",
    "vite": "^7.1.4"
  }
}
