{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "gh": "cross-env BASE_PATH=/telegram_mini_app/ vite build && npx gh-pages -d dist -b gh-pages",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.{ts,tsx,less,css,md}\" \"scripts/**/*.{mjs,js}\"",
    "preview": "vite preview",
    "images:webp": "node ./scripts/convert-images-to-webp.mjs",
    "images:webp:high": "WEBP_QUALITY=90 node ./scripts/convert-images-to-webp.mjs",
    "images:webp:lossless": "WEBP_LOSSLESS=true node ./scripts/convert-images-to-webp.mjs",
    "images:rename": "node ./scripts/rename-images.mjs"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/node": "^22.18.3",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.3.0",
    "less": "4.4.1",
    "prettier": "^3.6.2",
    "sharp": "0.34.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.42.0",
    "vite": "^7.1.4"
  }
}
